//Algoritmo 03
#include <stdio.h>
#include <stdlib.h>

int main() {

    //Seção de variáveis
    int base;
    int altura;
    
    //Exibindo as variáveis
    printf("Digite o valor da base de um retangulo: \n");
     scanf("%i", &base);
     
    printf("Digite o valor da altura: \n");
     scanf("%i", &altura);
     
    float area = base * altura;
    
    printf("Valor da área: %f \n", area);
    
    
    return 0;
    
}
